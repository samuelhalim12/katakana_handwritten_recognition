<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestDoneActivity">

    <TextView
        android:id="@+id/textSelamatUjian"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="8dp"
        android:gravity="center"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonKembaliMenuPilihUjian"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Kembali menu pilih ujian"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/textSelamatUjian"
        app:layout_constraintStart_toStartOf="@+id/textSelamatUjian"
        app:layout_constraintTop_toBottomOf="@+id/textSelamatUjian" />
</androidx.constraintlayout.widget.ConstraintLayout>